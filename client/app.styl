animation(nomanim,time,iteration,timing)
  -webkit-animation-name: nomanim //spin;
  -webkit-animation-duration: time//4000ms;
  -webkit-animation-iteration-count: iteration//infinite;
  -webkit-animation-timing-function: timing//linear;
  -moz-animation-name: nomanim//spin;
  -moz-animation-duration: time//4000ms;
  -moz-animation-iteration-count: iteration//infinite;
  -moz-animation-timing-function: timing//linear;
  -ms-animation-name: nomanim//spin;
  -ms-animation-duration: time//4000ms;
  -ms-animation-iteration-count: iteration//infinite;
  -ms-animation-timing-function: timing//linear;  
  animation-name: nomanim//spin;
  animation-duration: time//4000ms;
  animation-iteration-count: iteration//infinite;
  animation-timing-function: time//linear;
blink(time)
  -moz-animation blink time ease-in-out infinite normal /* Firefox */
  -webkit-animation blink time ease-in-out infinite normal /* Webkit */
  -ms-animation blink time ease-in-out infinite normal /* IE */
  animation blink time ease-in-out infinite normal /* Opera */

transitionos()
  -moz-transition:all arguments
  -webkit-transition:all arguments
  -o-transition:all arguments
  -ms-transition:all arguments
  transition:all arguments

@-webkit-keyframes blink {
    0% {
      opacity:1;
    }
    50% {
      opacity:0;
    }
    100% {
      opacity:1;
    }
  }

@-moz-keyframes blink {
  0% {
    opacity:1;
  }
  50% {
    opacity:0;
  }
  100% {
    opacity:1;
  }
}

@keyframes blink {
  0% {
    opacity:1;
  }
  50% {
    opacity:0;
  }
  100% {
    opacity:1;
  }
}

/*spin keyframes */
@-ms-keyframes spin {
    from { -ms-transform: rotate(0deg); }
    to { -ms-transform: rotate(360deg); }
}
@-moz-keyframes spin {
    from { -moz-transform: rotate(0deg); }
    to { -moz-transform: rotate(360deg); }
}
@-webkit-keyframes spin {
    from { -webkit-transform: rotate(0deg); }
    to { -webkit-transform: rotate(360deg); }
}
@keyframes spin {
    from {
        transform:rotate(0deg);
    }
    to {
        transform:rotate(360deg);
    }
}

