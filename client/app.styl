blink(time)
  -moz-animation blink time ease-in-out infinite normal /* Firefox */
  -webkit-animation blink time ease-in-out infinite normal /* Webkit */
  -ms-animation blink time ease-in-out infinite normal /* IE */
  animation blink time ease-in-out infinite normal /* Opera */

transitionos()
  -moz-transition:all arguments
  -webkit-transition:all arguments
  -o-transition:all arguments
  -ms-transition:all arguments
  transition:all arguments

@-webkit-keyframes blink {
    0% {
      opacity:1;
    }
    50% {
      opacity:0;
    }
    100% {
      opacity:1;
    }
  }

@-moz-keyframes blink {
  0% {
    opacity:1;
  }
  50% {
    opacity:0;
  }
  100% {
    opacity:1;
  }
}

@keyframes blink {
  0% {
    opacity:1;
  }
  50% {
    opacity:0;
  }
  100% {
    opacity:1;
  }
}



